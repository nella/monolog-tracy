{
	"name": "nella/monolog-tracy",
	"description": "Tracy BlueScreen handler for Monolog",
	"type": "library",
	"keywords": ["Monolog", "Tracy", "log", "logging", "blue screen"],
	"homepage": "https://github.com/pavelkucera/monolog-extensions",
	"license": ["MIT", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Pavel Kučera",
			"homepage": "https://github.com/pavelkucera"
		},
		{
			"name": "Patrik Votoček",
			"email": "patrik@votocek.cz",
			"homepage": "https://patrik.votocek.cz"
		}
	],
	"support": {
		"issues": "https://github.com/nella/monolog-tracy/issues",
		"source": "https://github.com/nella/monolog-tracy"
	},
	"autoload": {
		"psr-4": {
			"Nella\\MonologTracy\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Nella\\MonologTracy\\": "tests/"
		}
	},
	"require": {
		"php": "~5.6|~7.0",
		"monolog/monolog": "~1.1",
		"tracy/tracy": "~2.3.0|~2.4.0"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"nella/coding-standard": "4.2.1",
		"phing/phing": "2.14.0",
		"phpstan/phpstan": "^0.2",
		"phpunit/phpunit": "5.3.2"
	},
	"config": {
		"bin-dir": "bin"
	},
	"archive": {
		"exclude": [
			".gitattributes",
			".gitignore",
			"/build",
			"/tests"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
